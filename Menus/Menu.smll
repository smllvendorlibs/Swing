import Swing::Menus::MenuItem

type JMenu = "JMenu"

fn menu_new(title: String): JMenu => {
  val menu = null of JMenu
  java {
    "menu = new JMenu(title);"
  }
  menu
}

fn menu_add_menuitem(menu: JMenu, item: JMenuItem): Unit => {
  java {
    "menu.add(item);"
  }
  ()
}


fn menu_add_menu(menu: JMenu, other: JMenu): Unit => {
  java {
    "menu.add(other);"
  }
  ()
}

